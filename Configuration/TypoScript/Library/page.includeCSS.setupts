page {
	includeCSS {
		theme = EXT:theme_bootstrap_cleany/Resources/Private/Dyncss/import.less
		fontawesome = EXT:theme_bootstrap_cleany/Resources/Public/Stylesheetsfont-awesome.min.css
		fontawesome {
			excludeFromConcatenation = 1
			disableCompression = 1
		}
	}
}

[globalVar = LIT:1 = {$themes.mode.isProduction}]
	page {
		includeCSS {
			fontawesome = //maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css
			fontawesome {
				external = 1
			}
		}
	}
[global]